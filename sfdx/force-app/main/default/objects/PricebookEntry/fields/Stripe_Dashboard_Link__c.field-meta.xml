<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Stripe_Dashboard_Link__c</fullName>
    <description>Provides a direct link to the Stripe Price corresponding to this Salesforce Pricebook Entry.</description>
    <externalId>false</externalId>
    <formula>&quot;https://dashboard.stripe.com/&quot; &amp; IF($Setup.Stripe_Connection__c.Primary_Stripe_Account_Livemode__c == "live", &quot;&quot;, &quot;test/&quot;) &amp; &quot;id/&quot;&amp;Stripe_ID__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This link takes you to the Price Record in the Stripe Dashboard.</inlineHelpText>
    <label>Stripe Dashboard Link</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
